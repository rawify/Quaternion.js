{
    "name": "quaternion",
    "title": "Quaternion.js",
    "version": "1.6.0",
    "homepage": "https://github.com/rawify/Quaternion.js",
    "bugs": "https://github.com/rawify/Quaternion.js/issues",
    "description": "A rotation library using quaternions",
    "keywords": [
        "math",
        "quaternion",
        "number",
        "vector",
        "spherical interpolation",
        "parser",
        "slerp",
        "quat",
        "euler",
        "geometry",
        "3d",
        "vector",
        "matrix"
    ],
    "private": false,
    "main": "./dist/quaternion.js",
    "module": "./dist/quaternion.mjs",
    "types": "./quaternion.d.ts",
    "browser": "./dist/quaternion.min.js",
    "unpkg": "./dist/quaternion.min.js",
    "readmeFilename": "README.md",
    "exports": {
        ".": {
            "types": "./quaternion.d.ts",
            "require": "./dist/quaternion.js",
            "import": "./dist/quaternion.mjs"
        }
    },
    "repository": {
        "type": "git",
        "url": "git@github.com:rawify/Quaternion.js.git"
    },
    "funding": {
        "type": "github",
        "url": "https://github.com/sponsors/rawify"
    },
    "author": {
        "name": "Robert Eisele",
        "email": "robert@raw.org",
        "url": "https://raw.org/"
    },
    "license": "MIT",
    "engines": {
        "node": "*"
    },
    "scripts": {
        "build": "crude-build Quaternion",
        "test": "mocha tests/*.js"
    },
    "directories": {
        "example": "examples"
    },
    "devDependencies": {
        "crude-build": "^0.0.1",
        "mocha": "*"
    }
}
