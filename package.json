{
    "name": "quaternion",
    "title": "Quaternion.js",
    "version": "2.1.1",
    "description": "The RAW rotation library using quaternions",
    "homepage": "https://raw.org/book/algebra/quaternions/",
    "bugs": "https://github.com/rawify/Quaternion.js/issues",
    "keywords": [
        "math",
        "quaternion",
        "number",
        "vector",
        "spherical interpolation",
        "parser",
        "slerp",
        "quat",
        "euler",
        "geometry",
        "3d",
        "vector",
        "matrix"
    ],
    "private": false,
    "main": "./dist/quaternion.js",
    "module": "./dist/quaternion.mjs",
    "browser": "./dist/quaternion.min.js",
    "unpkg": "./dist/quaternion.min.js",
    "types": "./quaternion.d.ts",
    "exports": {
        ".": {
            "import": {
                "types": "./quaternion.d.mts",
                "default": "./dist/quaternion.mjs"
            },
            "require": {
                "types": "./quaternion.d.ts",
                "default": "./dist/quaternion.js"
            },
            "browser": {
                "types": "./quaternion.d.ts",
                "default": "./dist/quaternion.min.js"
            },
            "default": {
                "types": "./quaternion.d.ts",
                "default": "./dist/quaternion.js"
            }
        },
        "./package.json": "./package.json"
    },
    "typesVersions": {
        "<4.7": {
            "*": [
                "quaternion.d.ts"
            ]
        }
    },
    "sideEffects": false,
    "repository": {
        "type": "git",
        "url": "git@github.com:rawify/Quaternion.js.git"
    },
    "funding": {
        "type": "github",
        "url": "https://github.com/sponsors/rawify"
    },
    "author": {
        "name": "Robert Eisele",
        "email": "robert@raw.org",
        "url": "https://raw.org/"
    },
    "license": "MIT",
    "engines": {
        "node": "*"
    },
    "scripts": {
        "build": "crude-build Quaternion",
        "test": "mocha tests/*.js"
    },
    "directories": {
        "example": "examples"
    },
    "devDependencies": {
        "crude-build": "^0.1.2",
        "mocha": "*"
    }
}